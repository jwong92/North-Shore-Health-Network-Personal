@model IEnumerable<WebApplication1.Models.image>
@foreach (var item in Model)
{
    <img class="mr-3 article_images" src="~/News_Images/@item.news.id.ToString()/@item.img_src.ToString()" alt="Generic placeholder image">
    @Html.DisplayFor(model => item.caption)
}



